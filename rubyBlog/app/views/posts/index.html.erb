<h1>Nuevo_Posts</h1>


<thead>
<tr>
<th>Titulo</th>
<th>Cuerpo</th>
<th>Autor</th>
<th>Fecha de creación</th>
<th>Última modificación</th>
</tr>
</thead>
<tbody>
<% @posts.each do |post| %>
<tr>
<td><%= post.title %></td>
<td><%= post.body %></td>
<td><%= post.author %></td>
<td><%= post.created_at %></td>
<td><%= post.updated_at %></td>
<td><%= link_to "Editar", edit_post_path(post) %></td>
<td><%= link_to "Eliminar", post_path(post), data: {method: "DELETE"} %></td>
</tr>
<% end%>
</tbody>
</table>

<p>Find me in app/views/posts/index.html.erb</p>
